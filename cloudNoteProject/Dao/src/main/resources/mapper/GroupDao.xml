<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zjy.cloudnote.dao.GroupDao">
    <select id="queryGroup" resultType="com.zjy.cloudnote.entity.Group">
        SELECT group_id,group_name,school_id
        FROM cn_group
        ORDER BY group_id
        DESC
    </select>
</mapper>