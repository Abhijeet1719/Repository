#left_nav_ul {
    width: 100%;
}

#left_nav_ul li {
    display: block;
    /*position: relative;*/
    width: 100%;
}

#left_nav_ul li a {
    display: inline-block;
    width: 100%;
    height: 40px;
    line-height: 40px;
}

#left_nav_ul li a:hover,
#left_nav_ul li a.end {
    /*background: #eaedf1;*/
    color: #286090;
}

#left_nav_ul li a i {
    display: inline-block;
    float: left;
    margin-top: 15px;
    margin-right: 6px;
    width: 8px;
    height: 8px;
    overflow: hidden;
    background: url(../image/project/right.png) no-repeat;
}

#left_nav_ul li.selected > a i {
    background: url(../image/project/down.png) no-repeat;
}

#left_nav_ul li a i.end {
    background: none;
}

#left_nav_ul li a h6 {
    font-size: 14px;
    font-weight: 400;
    text-indent: 0;
    text-overflow:ellipsis;
    white-space:nowrap;
}
#left_nav_ul ul {
    display: none;
}
#left_nav_ul li.selected > a h6 {
    color: #286090;
}

#left_nav_ul li a > .menu-btns {
    display: none;
    height: 16px;
    position: absolute;
    top: 14px;
    right: 15px;
}

#left_nav_ul li a > .menu-btns .a-btn{
    margin-left:6px;
}

#left_nav_ul li a > .menu-btns > span {
    display: inline-block;
    float: left;
    width: 16px;
    height: 16px;
}

#left_nav_ul li a:hover > .menu-btns {
    display: inline-block;
}
.menu-info-c{
    float: left;
    display: block;
    width: 255px;
    position: absolute;
    right: 15px;
}
.menu-info-c *{
    float: left;
}
span.split_screen,span.video_screen{
    display: inline-block !important;
    /*margin-top: 9px;*/
    width:26px;
    height: 21px;
    /*float:right;*/
    margin-left: 10px;
}
span.split_screen{
    background: url(../image/split_screens_ic.png) no-repeat ;
    /*margin-left: 20px;*/
}
span.video_screen{
    background: url(../image/video_screens_ic.png) no-repeat ;
}

