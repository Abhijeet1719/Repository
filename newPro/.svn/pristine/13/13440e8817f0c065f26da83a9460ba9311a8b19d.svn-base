/*弹出框*/
.popupbtn{
	width: 700px; height: 50px; position: relative;
}
.popupbtn>div{
	position:absolute;right:30px;width:70px;float:left;background: none repeat scroll 0% 0% #46AF4B;color: #FFFFFF;filter: none;text-align:center;font-size:14px;vertical-align:middle;line-height:30px;border-radius:3px;cursor:pointer
}
.popupbtn .right{ 
	right: 120px;
	background-color: #999;
}
.popupbtn>div:hover{ box-shadow: 1px 1px 10px #777 inset; }
.popupbtn>div:active{ box-shadow: 1px 1px 10px #444 inset; }

.tempifram{
	width:700px;height:340px;overflow:hidden;border:none;
}
	
.tempifram .popup{ left: 0px; top: 0px; }


.publishdiv{ position:absolute;top:50%;left:50%;z-index:999;  }

}
.publishdiv-bg{position:absolute;top:0;left:0;width:100%;height:100%;background-color:rgba(0,0,0,0.3);z-index:99;}
.publishdiv-in{ position:relative;background-color:#ffffff;width:700px;border-radius:2px; left:0px; top:0px; }
.closebtn{position: absolute;border-radius: 500px;top: 6px;right: 10px;z-index: 50;font-size: 20px;color: #FFF;cursor: pointer;background-color: #009900;padding: 2px 10px;}
.closebtn:hover{background-color: #0cbb00;padding: 3px 12px;top: 5px;right: 8px; }
.btnbar-bg{width:100%;height:50px;}
.btnbar{cursor:pointer;margin-right:10px;margin-top: 8px;float:right;background: none repeat scroll 0% 0% #46AF4B;color: #FFFFFF;filter: none;text-align:center;vertical-align:middle;border-radius:3px;padding:5px 16px;}
.cancel{ background-color: #aaa; }
.treeDemo{ background-color:#FFFFFF; }
.treeDemo tr{  color:#FFF; background-color:#46AF4B; height:44px; }
