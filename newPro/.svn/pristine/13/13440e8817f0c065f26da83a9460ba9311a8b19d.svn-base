/** baseline start **/
.baseline_horizontal {
  /*水平*/
  position: absolute;
  height: 0;
  border-top: 1px solid red;
  display: none;
  z-index: 3; }

.baseLine_vertical {
  /*垂直*/
  position: absolute;
  width: 0;
  border-left: 1px solid red;
  display: none;
  z-index: 3; }

/** baseline end **/
/** edite mask start **/
.svg_mask {
  position: absolute;
  z-index: 500; }

.editEle_mask, .ctrlEle_mask {
  position: absolute;
  z-index: 500; }

.editEle_mask {
  border: 1px dashed gray;
  margin-top: -1px;
  margin-left: -1px; }

#editEle_mask_1 {
  pointer-events: none; }

.ctrlEle_mask {
  /*background-color: rgba(2,83,175,0.3);*/
  border: 1px dashed gray;
  margin-top: -1px;
  margin-left: -1px; }

.editEle_mask .editEle_TL_resize, .editEle_mask .editEle_T_resize, .editEle_mask .editEle_TR_resize, .editEle_mask .editEle_L_resize,
.editEle_mask .editEle_BL_resize, .editEle_mask .editEle_B_resize, .editEle_mask .editEle_R_resize, .editEle_mask .editEle_BR_resize {
  position: absolute;
  width: 8px;
  height: 8px;
  background-color: gray;
  border-radius: 4px;
  pointer-events: auto; }

.editEle_mask .editEle_TL_resize {
  cursor: nw-resize;
  top: -4px;
  left: -4px; }

.editEle_mask .editEle_T_resize {
  cursor: n-resize;
  left: 50%;
  top: -4px;
  margin-left: -4px; }

.editEle_mask .editEle_TR_resize {
  cursor: ne-resize;
  right: -4px;
  top: -4px; }

.editEle_mask .editEle_L_resize {
  cursor: w-resize;
  left: -4px;
  top: 50%;
  margin-top: -4px; }

.editEle_mask .editEle_BL_resize {
  cursor: sw-resize;
  left: -4px;
  bottom: -4px; }

.editEle_mask .editEle_B_resize {
  cursor: s-resize;
  bottom: -4px;
  left: 50%;
  margin-left: -4px; }

.editEle_mask .editEle_R_resize {
  cursor: e-resize;
  right: -4px;
  top: 50%;
  margin-top: -4px; }

.editEle_mask .editEle_BR_resize {
  cursor: se-resize;
  bottom: -4px;
  right: -4px; }

