<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
    <package name="user" namespace="/user" extends="frontend-default">
        <action name="User_*" class="com.honghe.recordweb.action.frontend.user.UserAction" method="{1}">
            <result name="loginSuccessuser" type="redirect">/user/User_userList.do</result>
            <result name="loginSuccessrole" type="redirect">/role/Role_roleList.do</result>
            <result name="loginSuccessresource" type="redirect">/resource/Resource_resourceList.do</result>
            <result name="loginSuccessdmanager" type="redirect">/dmanager/DManager_getHostGroup.do</result>
            <result name="loginSuccesstimeplan" type="redirect">/timeplan/Timeplan_plan.do</result>
            <result name="loginSuccessdevice" type="redirect">/hostgroup/Hostgroup_management.do</result>
            <result name="loginSuccessviewclass" type="redirect">/viewclass/Viewclass_getHostGroup.do</result>
            <result name="loginSuccessviewclassvice" type="redirect">/hostgroup/Hostgroup_management.do</result>
            <result name="loginSuccesshtprobjector" type="redirect">/htprojector/HTProjector_getHostGroup.do</result>
            <result name="loginSuccesshtprobjectorvice" type="redirect">/hostgroup/Hostgroup_management.do</result>
            <result name="loginSuccesshhtwbobjector" type="redirect">/hhtwhiteboard/HHTWhiteboard_getHostGroup.do</result>
            <result name="loginSuccesshhtwbobjectorvice" type="redirect">/hostgroup/Hostgroup_management.do</result>
            <result name="loginSuccessdefault">/deny.jsp</result>
            <result name="loginError">/login.jsp</result>
            <result name="userlist" type="redirect">http://${localAddr}:7071/user/users.jsp?id=2&amp;logOut=${logOutUrl}&amp;userId=${userId}&amp;userName=${userName}&amp;userRealName=${userRealName}</result>
            <!--<result name="userlist">/pages/frontend/user/user_list.jsp</result>-->
            <result name="userdetail">/pages/frontend/user/user_add.jsp</result>
            <result name="usersearch">/pages/frontend/user/user_searchlist.jsp</result>
            <result name="authorityDeny">/pages/frontend/404.jsp</result>
            <result name="logout">/login.jsp</result>
        </action>
    </package>
</struts>