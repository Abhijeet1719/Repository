<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
        <package name="video" namespace="/video" extends="frontend-default">
                <action name="Video_*" class="com.honghe.recordweb.action.frontend.video.VideoAction" method="{1}">
                        <result name="videoList">/pages/frontend/video/video.jsp</result>
                        <result name="cameraInfo">/pages/frontend/video/video_cameraInfo.jsp</result>
                        <result name="videoftp">/pages/frontend/video/video_ftp.jsp</result>
                        <result name="videoplay">/pages/frontend/video/video_liveplay.jsp</result>
                        <result name="videolistip">/pages/frontend/video/video_ip.jsp</result>
                </action>
        </package>
</struts>