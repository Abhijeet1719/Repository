/*
	===================================================
	2014-09-24  12:22
	集控后台样式表--全局公用部分
	===================================================
*/
HTML, body, div, h1, h2, h3, h4, h5, h6, ul, ol, dl, li, dt, dd, p, blockquote,
pre, form, fieldset, table, th, td {
	margin:0px 0px;
	padding:0px 0px;
	font-size:14px;
	font-family:"宋体","微软雅黑","黑体";
	}
input::-ms-clear{display:none;}
input::-ms-reveal{display:none;}
img{
	border:none;
	}
a:link{
	color:#333333;
	text-decoration:none;
	}
a:visited{
	color:#333333;
	text-decoration:none;
	}
a:hover{
	color:#333333;
	text-decoration:none;
	}
a:active{
	color:#333333;
	text-decoration:none;
	}
li{
	list-style-type:none;
	}
/*
	===================================================
	2014-09-24  12:22
	集控后台样式表--公用部分
	===================================================
*/
.public{
	margin:0 auto;
	min-width: 1200px;
	width: 100%;
	overflow: hidden;
	}
.public_head{
	border-bottom:2px solid #28b779;
	background: #333333;
	float: left;
	height: 70px;
	position: relative;
	width: 100%;
	}
.public_head_logo{
	background: url(../../image/project/logo.png) 0 0 no-repeat;
	height: 31px;
	margin: 20px auto 0 auto;
	width: 316px;
	}
.public_adminout{
	color: #fff;
	height:18px;
	line-height: 18px;
	position: absolute;
	right: 30px;
	top: 30px;
}
.public_admin{
	border-right:1px solid #fff;
	margin-right: 10px;
	padding-right:10px;
	}
.public_left{
	float: left;
	min-height: 636px;
	position: relative;
	width: 258px;
	}
.public_right{
	border-left:2px solid #28b779;
	background: #f2f2f2;
	min-height: 636px;
	position: relative;
	}
.public_right_center{
	/*height: 550px;*/
	margin: 0 auto;
	min-width: 845px;
	padding-top: 20px;
	width: 100%;
	}
.foot{
	border-top:2px solid #28b779;
	background: #333333;
	/*float: left;*/
	height: 30px;
	min-width: 1200px;
	width: 100%;
	}
.foot_center{
	color: #fff;
	line-height: 30px;
	height: 30px;
	left: 50%;
	margin-left:-300px;
	position: relative;
	text-align: center;
	width: 600px;
}
/*
	===================================================
	2014-09-24  12:22
	集控后台样式表--登录页面
	===================================================
*/
.public_head_login{
	border-bottom:2px solid #28b779;
	background: #333333;
	float: left;
	height: 204px;
	position: relative;
	width: 100%;
	}
.public_head_logo_login{
	background: url(../../image/project/logo_login.png) left top no-repeat;
	height: 51px;
	margin: 68px auto 0 auto;
	width: 568px;
	}
.public_login_form{
	float: left;
	min-height: 466px;
	position: relative;
	width: 100%;
	}
.public_login_form_center{
	height: 190px;
	margin: 125px auto 0 auto;
	width: 386px;
	}
.public_login_input{
	border:1px solid #28b779;
	height: 35px;
	margin-bottom: 30px;
	text-indent: 18px;
	width: 358px;
	}
.public_login_btn{
	border:none;
	background: #28b779;
	border-radius: 3px;
	color: #fff;
	cursor: pointer;
	font-size: 24px;
	float: right;
	height: 35px;
	line-height: 35px;
	outline: none;
	text-align: center;
	text-indent: 0;
	width: 100px;
	}
/*
	===================================================
	2014-09-24  12:22
	集控后台样式表--树形菜单
	===================================================
*/
.tree_title{
	border-bottom: 1px solid #e4e4e4;
	cursor:pointer;
	float: left;
	line-height:33px;
	text-indent: 25px;
	width:260px;
	}
.tree_titlea{
	cursor:pointer;
	float: left;
	height: 33px;
	line-height:33px;
	overflow: hidden;
	text-indent: 25px;
	width:260px;
	}
.tree_title_close{background: url(../../image/project/n_icon_140924.png) 12px -79px no-repeat;}
.tree_title_open{background: url(../../image/project/n_icon_140924.png) -173px -27px no-repeat;}
.tree_content{
	display:none;
	float: left;
	width:260px;
	}
.tree_content a{
	display:block;
	float:left;
	line-height:33px;
	width:260px;
	}
.tree_selected{
	color:#fff;
	cursor: pointer;
	display:block;
	float:left;
	line-height:33px;
	overflow:hidden;
	text-overflow:ellipsis;
	white-space:nowrap;
	width:260px;
	}	
