body,ul,li,p { padding:0; margin:0; border:0; } 
body { font: 12px  "Microsoft YaHei",Arial,simsun,tahoma,Srial,helvetica,sans-serif; -webkit-user-select:none; -webkit-text-size-adjust:none; }
.editItem_time1 { border-radius:5px; border-style:solid;}
.editItem_time1 .wrap{ position:absolute; left:0; top:0; width:100%; height:100%; text-align:center; font-family:"Microsoft YaHei",Arial,tahoma,Srial,helvetica; overflow:hidden; }
.editItem_time1 .wrap table{ table-layout:fixed; width:100%; height:100%;}
.editItem_time1 .wrap .time{ font-size:28px; width:60%; }
.editItem_time1 .wrap .date{ font-size:11px; text-align:left;}
