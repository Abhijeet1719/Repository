<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
        <package name="place" namespace="/place" extends="frontend-default">
                <action name="Place_*" class="com.honghe.recordweb.action.frontend.place.PlaceAction" method="{1}">
                        <result name="placemanagement" type="redirect">http://${localAddr}:7071/place/#/place?id=1&amp;logOut=${logOutUrl}&amp;userId=${userId}&amp;userName=${userName}&amp;userRealName=${userRealName}</result>
                </action>
        </package>
</struts>