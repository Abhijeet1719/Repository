<div id="@#+bid+#@" class="item" style="left:@#+x+#@px;top:@#+y+#@px;transform:scale(@#+scX+#@,@#+scY+#@);-webkit-transform:scale(@#+scX+#@,@#+scY+#@);z-index:@#+idx+#@;">
	<div class="editItem_countdown" style="position:relative;">
			<div class="COUNTDOWN_COMP_SRC">
				<input type="hidden" value="@#+cdms+#@">
				<span class="countdowntime" id="cddd" style="color:@#+fontcolor+#@">@#+tymdhmsd+#@</span>
				<span class="countdowndate" style="color:@#+fontcolor+#@">天</span>
				<span class="countdowntime" id="cddh" style="color:@#+fontcolor+#@">@#+tymdhmsh+#@</span>
				<span class="countdowndate" style="color:@#+fontcolor+#@">时</span>
				<span class="countdowntime" id="cdddm" style="color:@#+fontcolor+#@">@#+tymdhmsm+#@</span>
				<span class="countdowndate" style="color:@#+fontcolor+#@">分</span>
				<span class="countdowntime" id="cdds" style="color:@#+fontcolor+#@">@#+tymdhmss+#@</span>
				<span class="countdowndate" style="color:@#+fontcolor+#@">秒</span>
			</div>
	</div>
</div>

