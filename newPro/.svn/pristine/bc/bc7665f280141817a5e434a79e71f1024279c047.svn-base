package com.honghe.recordweb.service.frontend.tresource;

public enum ResourceStatusFlag {
	   Uploaded("1"),//上传完成
	   Converting("2"),//正在转换
	   Converted("3"),//转换完成
	   ConvertError("4"),//转换错误
	   Illegal("5"); //非法格式
	

	    private String flag; 

	    ResourceStatusFlag(String flag) { 
	        this.flag = flag; 
	    } 

	    public String getFlag() { 
	        return flag; 
	    } 
}
