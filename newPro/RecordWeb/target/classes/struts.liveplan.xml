<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
    <package name="liveplan" namespace="/liveplan" extends="frontend-default">
        <action name="Liveplan_*" class="com.honghe.recordweb.action.frontend.liveplan.LiveplanAction" method="{1}">
            <result name="liveplan">/pages/frontend/liveplan/liveplan.jsp</result>
            <result name="classtimeploy" type="redirect">/settings/Settings_timeploy.do</result>
            <result name="liveplanlist">/pages/frontend/liveplan/liveplanlist.jsp</result>
        </action>
    </package>
</struts>