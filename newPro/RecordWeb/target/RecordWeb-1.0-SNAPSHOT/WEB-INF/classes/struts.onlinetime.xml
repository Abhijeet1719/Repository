<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
    <package name="onlinetime" namespace="/onlinetime" extends="frontend-default">
        <action name="Onlinetime_*" class="com.honghe.recordweb.action.frontend.onlinetime.OnlineTimeAction" method="{1}">
            <result name="report">/pages/frontend/onlinetime/onlinetime.jsp</result>
            <result name="deviceloglist">/pages/frontend/report/deviceloglist.jsp</result>
            <result name="reportAjax">/pages/frontend/onlinetime/onlinetimeAjax.jsp</result>
            <result name="viewChart">/pages/frontend/onlinetime/onlinechart.jsp</result>
            <result name="softinuse">/pages/frontend/onlinetime/softinuse.jsp</result>
            <result name="export_success" type="stream" >
                <param name="contentLength">${fileLength}</param>
                <param name="contentDisposition">attachment;filename="${fileName}"</param>
                <param name="inputName">inputStream</param>
            </result>
        </action>
    </package>
</struts>