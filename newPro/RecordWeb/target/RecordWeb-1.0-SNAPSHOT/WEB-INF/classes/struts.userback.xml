<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
    <package name="userback" namespace="/userback" extends="backend-default">
        <action name="Userback_*" class="com.honghe.recordweb.action.project.user.UserBackAction" method="{1}">
            <result name="loginBackSuccess">/pages/project/index.jsp</result>
            <result name="userlistback">/pages/project/user_list.jsp</result>
            <result name="userdetail">/pages/project/user_add.jsp</result>
            <result name="authorityDeny">/deny.jsp</result>
            <result name="logout">/pages/project/login.jsp</result>
        </action>
    </package>
</struts>