<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
        <package name="devicecode" namespace="/devicecode" extends="frontend-default">
                <action name="Devicecode_*" class="com.honghe.recordweb.action.frontend.devicecode.DevicecodeAction" method="{1}">
                        <result name="codelist">/pages/frontend/devicecode/codelist.jsp</result>
                        <result name="ccodelist">/pages/frontend/devicecode/ccodelist.jsp</result>
                        <result name="delsuccess">/codeList.do</result>
                        <result name="codeadd">/pages/frontend/devicecode/codeadd.jsp</result>
                        <result name="codesearch">/pages/frontend/devicecode/codesearch.jsp</result>
                        <result name="authorityDeny">/deny.jsp</result>
                </action>
        </package>
</struts>