<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
    <package name="deviceview" namespace="/deviceview" extends="frontend-default">
        <action name="Deviceview_*" class="com.honghe.recordweb.action.frontend.deviceview.DeviceViewAction"
                method="{1}">
            <result name="devicelist">/pages/frontend/deviceview/deviceview.jsp</result>
            <result name="devicesearch">/pages/frontend/deviceview/deviceviewsearch.jsp</result>
            <result name="export_success" type="stream">
                <param name="contentLength">${fileLength}</param>
                <param name="contentDisposition">attachment;filename="${fileName}"</param>
                <param name="inputName">inputStream</param>
            </result>


        </action>
    </package>
</struts>